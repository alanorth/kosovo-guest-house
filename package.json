{
  "name": "kosovo-guest-house",
  "version": "0.0.1",
  "description": "Website for a fictional guest house in the Bulgarian village of Kosovo",
  "main": "index.js",
  "scripts": {
    "build:css": "node-sass --output-style compressed scss/style.scss | cleancss > css/style.css",
    "build:js": "cp node_modules/bootstrap/dist/js/bootstrap.min.js node_modules/jquery/dist/jquery.min.js js",
    "build": "npm run build:css & npm run build:js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alanorth/kosovo-guest-house.git"
  },
  "keywords": [
    "bulgaria",
    "travel"
  ],
  "author": "Alan Orth",
  "license": "GPL-3.0",
  "engines": {
    "node": "4.5.0"
  },
  "dependencies": {
    "bootstrap": "4.0.0-alpha.3",
    "jquery": "3.0.0"
  },
  "devDependencies": {
    "clean-css": "^3.4.19",
    "node-sass": "^3.8.0"
  }
}
